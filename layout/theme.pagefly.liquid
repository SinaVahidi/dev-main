    {%- endif -%}

    {% include 'seo-title' %}
    {%- comment smartseo_integration_begin_meta_data -%}<title>{{ seo_title }}</title>{%- endcomment smartseo_integration_end_meta_data -%}

    {%- if page_description -%}
    {%- comment smartseo_integration_begin_meta_data -%}<meta name="description" content="{{ page_description | escape }}">{%- endcomment smartseo_integration_end_meta_data -%}
    {%- endif -%}

    {% include 'social-meta-tags' %}
    {% include 'font-face' %}

    <script type="application/json" data-theme-information>
      {
        "locale": {
          "productFlavour": "{{ 'general.subscription_page.product_flavours' | t }}",
          "productOption": "{{ 'general.subscription_page.product_options' | t }}"
        }
      }
    </script>  

    <script>
      document.documentElement.className = document.documentElement.className.replace('no-js', 'js');

      window.theme = window.theme || {};
      theme.strings = {
        addToCart: {{ 'products.product.add_to_cart' | t | json }},
        soldOut: {{ 'products.product.sold_out' | t | json }},
        unavailable: {{ 'products.product.unavailable' | t | json }},
        stockLabel: {{ 'products.product.stock_label' | t: count: '[count]' | json }},
        cartSavings: {{ 'cart.general.savings_html' | t: savings: '[savings]' | json }},
        cartEmpty: {{ 'cart.general.empty' | t | json }},
        cartTermsConfirmation: {{ 'cart.general.terms_confirm' | t | json }}
      };
      theme.settings = {
        dynamicVariantsEnable: true,
        dynamicVariantType: {{ settings.variant_type | json }},
        cartType: {{ settings.cart_type | json }},
        currenciesEnabled: {{ settings.enable_currencies | json }},
        nativeMultiCurrency: {{ shop.enabled_currencies.size }} > 1 ? true : false,
        moneyFormat: {{ shop.money_format | json }},
        inventoryThreshold: {{ settings.inventory_threshold }},
        quickView: {{ settings.quick_shop_enable }},
        themeVersion: "4.3.0"
      };
    </script>

    {{ content_for_header }} 
    {%- include 'bold-common' -%} 
    {%- include 'bold-product' with product, hide_action: 'header' -%}  
      
    {{ 'bold.css' | asset_url | stylesheet_tag }} 
    {{ 'ffc.css' | asset_url | stylesheet_tag }}

    <script src="{{ 'vendor-scripts-v5.js' | asset_url | split: '?' | first }}" defer="defer"></script>

    {% if settings.enable_currencies %}
      {% unless shop.enabled_currencies.size > 1 %} 
      <script src="/services/javascripts/currencies.js" defer="defer"></script> 
      <script src="{{ 'currencies.jquery.min.js' | asset_url | split: '?' | first }}" defer="defer"></script> 
      {% endunless %}     
      <link rel="stylesheet" href="{{ 'currency-flags.css' | asset_url }}"> 
    {% endif %}

    <link rel="stylesheet" href="{{ 'currency-flags.css' | asset_url }}"> 
    <script src="{{ 'theme.js' | asset_url }}" defer="defer"></script>  
    
    {%- if template contains 'customers' -%}  
    <script src="{{ 'shopify_common.js' | shopify_asset_url }}" defer="defer"></script> 
    {%- endif -%} 
    
